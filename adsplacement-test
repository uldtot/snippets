<script>
jQuery( document ).ready(function($) {
	$(window).resize(function() {

	 resizeAds()

	});

resizeAds();

	function resizeAds() {

		if ($(this).width() < "1500") {

	        $('.floatingads').hide();

	      } else {

	        $('.floatingads').show();
	        // Calculate div positions
	        var windowWidth = $(this).width();
	        var leftSpace = ((windowWidth-1140)/2);
	        var rightSpace = ((windowWidth-1140)/2);
	          $(".floatingads.left").css("left", leftSpace-160);
	          $(".floatingads.right").css("right", rightSpace-160);

	        }

	}
});
</script>
		<style>
		@media only screen and (max-width: 1500px) {
	  .floatingads {
	    display:none;
	  }
	}
		</style>
